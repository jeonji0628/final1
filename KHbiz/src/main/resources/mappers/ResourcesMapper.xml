<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ResourcesMapper">
	<select id="allselect" resultType="com.KHbiz.resources.ResourcesDTO">
		select * from resources 
	</select>
	<insert id="bookresource">
		insert into res_status values(#{code}, #{startday}, #{lastday}, #{id}, #{name})
	</insert>
	<select id="booklist" resultType="com.KHbiz.resources.StatusDTO">
		select * from res_status where code=#{code}
	</select>
</mapper>